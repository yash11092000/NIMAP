@model System.Data.DataSet

@if (Model.Tables[0].Rows.Count > 0)
{
    for (int i = 0; i < Model.Tables[0].Rows.Count; i++)
    {
        <tr>
            <td>@Model.Tables[0].Rows[i]["ProductId"]</td>
            <td>@Model.Tables[0].Rows[i]["ProductName"]</td>
            <td>@Model.Tables[0].Rows[i]["CategoryId"]</td>
            <td>@Model.Tables[0].Rows[i]["CategoryName"]</td>
            <td width="5%" class="text-center font-bold text-primary Icon" onclick="EditProduct('@Model.Tables[0].Rows[i]["ProductId"]')"> &#9998;</td>
            <td width="5%" class="text-center font-bold text-danger Icon" onclick="DeleteProduct('@Model.Tables[0].Rows[i]["ProductId"]')">&#215;</td>
        </tr>

    }
}
else
{
    <tr>
        <td colspan="6" class="text-center">No Record Found</td>
    </tr>

}


